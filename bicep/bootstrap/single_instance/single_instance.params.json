{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualNetworks": {
            "value": [
                {
                    "virtualNetworkName": "ora01vnet",
                    "addressPrefixes": [
                        "10.0.0.0/16"
                    ]
                }
            ]
        },
        "vnetSubnets": {
            "value": [
                {
                    "virtualNetworkName": "ora01vnet",
                    "subnetName": "ora01subnet",
                    "addressPrefix": "10.0.0.0/24"
                }
            ]
        },
        "networkSecurityGroups": {
            "value": [
                {
                    "networkSecurityGroupName": "ora01nsg"
                }
            ]
        },
        "networkInterfaces": {
            "value": [
                {
                    "networkInterfaceName": "ora01nic0"
                }
            ]
        },
        "publicIPAddresses": {
            "value": [
                {
                    "publicIPAddressName": "pip01"
                },
                {
                    "publicIPAddressName": "pip02"
                },
                {
                    "publicIPAddressName": "pip03"
                }
            ]
        },
        "dataDisks": {
            "value": [
                {
                    "diskName": "ora01disk0",
                    "size": 256,
                    "type": "Premium_LRS",
                    "lun": 0,
                    "virtualMachineName": "ora01",
                    "avZone": "1"
                }
            ]
        },
        "virtualMachines": {
            "value": [
                {
                    "virtualMachineName": "ora01",
                    "adminUserName": "oracle",
                    "sshPublicKey": "ssh-rsa ",
                    "avZone": "1",
                    "vmSize": "Standard_D4s_v5"
                }
            ]
        },
        "tags": {
            "value": {
                "workload": "oracle"
            }
        },
        "oracleImageReference": {
            "value": {
                "publisher": "oracle",
                "offer": "oracle-database-19-3",
                "sku": "oracle-database-19-0904",
                "version": "latest"
            }
        }
    }
}